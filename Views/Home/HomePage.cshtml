@{
    ViewData["Title"] = "Home Page";
}

<div id="carouselExampleInterval" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
        <div id="carousel" class="carousel-item active" data-interval="10000">
            <img src="~/images/dog1.jpg" class="d-block w-100" alt="...">
        </div>
        <div id="carousel" class="carousel-item" data-interval="2000">
          <img src="~/images/dog2.jpg" class="d-block w-100" alt="...">
        </div>
        <div id="carousel" class="carousel-item">
            <img src="~/images/dog3.jpg" class="d-block w-100" alt="...">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleInterval" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleInterval" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

<div class="row mt-4">
    <a asp-action="AllItems" asp-controller="Item">
    <div class="card border-success mb-3 col" style="width: 23.5rem; height: 10rem;">
        <div class="card-body">
            <h4 class="card-title">Products</h4>
        </div>
    </div>
    </a>
    <a>
    <div class="card border-success mb-3 col" style="width: 23.5rem; height: 10rem;">
        <div class="card-body">
            <h4 class="card-title">About the Company</h4>
        </div>
    </div>
    </a>
    <a asp-action="ProjectForm" asp-controller="Item">
    <div class="card border-success mb-3 col" style="width: 23.5rem; height: 10rem;">
        <div class="card-body">
            <h4 class="card-title">Start Your Own Project!</h4>
        </div>
    </div>
    </a>
</div>

